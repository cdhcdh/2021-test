<h1>{{ title }}</h1>

<table><!-- 테이블 시작 -->
    <tr><!--tr은 행을 의미합니다.-->
        <td>Fname</td><!--td는 열을 의미합니다-->
        <td>Minit</td>
        <td>Lname</td>
        <td>Ssn</td>
        <td>Bdate</td>
        <td>Address</td>
        <td>Sex</td>
        <td>Salary</td>
        <td>Super_ssn</td>
        <td>Dno</td>
    </tr>
    {{#each emp_res}}<!--#each /each를 사용해 반복하면서 테이블을 채웁니다.-->
    <form method="post"><!--form method을 사용해서 값을 입력, update/employee로 값을 넘겨줍니다-->
    <tr>
        <td>{{Fname}}</td>
        <td>{{Minit}}</td>
        <td>{{Lname}}</td>
        <td>{{Ssn}}</td>
        <td>{{Bdate}}</td>
        <td>{{Address}}</td>
        <td>{{Sex}}</td>
        <td><input type="text" name="salary" placeholder={{Salary}}></td>
        <td>{{Super_ssn}}</td>
        <td>{{Dno}}</td>
        <td><input type="submit" value="수정" formaction="/update/employee"></td>
    </tr>
    </form>
    {{/each}}
</table>